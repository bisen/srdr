			<% end %>
			<!--<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/baselines">Baseline Data</a></li> -->
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/question_builder?model=baseline_characteristic">Baseline Data</a></li>
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/outcomes">Outcomes</a></li>
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/question_builder?model=outcome_detail">Outcome Details</a></li>
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/adverse">Adverse Events</a></li>
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/quality">Quality</a></li>
      <% if current_user.is_admin? %>
      <li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/question_builder?model=quality_detail">Q2</a>
      </li>
      <% end %>
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/finalize">Finalize</a></li>	
			
		<% end %>
	</ul>
	<div id="tabs-1"><br/>
		<%= render "extraction_forms/new" %>
	</div>

</div>


