<div id="tabs">
	<ul class='ef-tabs'>
		<li class='ef-tab-color'><a href="#tabs-1">Title and KQs</a></li>
		<% if !@extraction_form.id.nil? %>
			<!--<li><a href="extraction_forms/<%= @extraction_form.id.to_s %>/import">Options</a></li>		-->
			<li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/publications">Publications</a></li>
			<!-- <li class='ef-tab-color'><a href="/projects/<%= @project.id.to_s %>/extraction_forms/<%= @extraction_form.id.to_s %>/design">Study Design</a></li>-->
